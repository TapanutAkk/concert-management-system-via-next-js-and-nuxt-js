datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Concert {
  id          String   @id @default(uuid())
  name        String   @unique
  totalSeats  Int
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}